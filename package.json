{
  "name": "mina-ledger-js",
  "version": "1.0.4",
  "description": "JS API for Mina App (Ledger Nano S/X)",
  "license": "Apache-2.0",
  "author": "Carbonara <gh@carbonara.science>",
  "keywords": [
    "Ledger",
    "Javascript",
    "Mina",
    "Minaprotocol"
  ],
  "bugs": {
    "url": "https://github.com/nerdvibe/mina-ledger-js/issues"
  },
  "homepage": "https://github.com/nerdvibe/mina-ledger-js",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerdvibe/mina-ledger-js.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "build": "tsc --outDir dist/",
    "test": "echo \"No test specified for now\"",
    "start-web": "cross-env NODE_ENV=development parcel index.html --https",
    "build-web": "cross-env NODE_ENV=production parcel build index.html --no-minify --public-url /docs --out-dir docs"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@ledgerhq/hw-app-eth": "^5.47.3",
    "@ledgerhq/hw-transport-u2f": "^5.36.0-deprecated",
    "@ledgerhq/hw-transport-webusb": "^5.46.0",
    "buffer": "6.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cross-env": "^5.2.0",
    "husky": "5.1.3",
    "parcel-bundler": "^1.12.3",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "ts-node-dev": "1.0.0",
    "typescript": "4.2.3"
  }
